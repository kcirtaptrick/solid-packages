{
  "name": "solid-overlays",
  "version": "0.4.10",
  "description": "Overlays for SolidJS",
  "license": "MIT",
  "author": "Patrick Wang <patrick.bohan.wang@gmail.com>",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kcirtaptrick/solid-packages.git"
  },
  "homepage": "https://github.com/kcirtaptrick/solid-packages/tree/master/packages/solid-overlays#readme",
  "bugs": {
    "url": "https://github.com/kcirtaptrick/solid-packages/issues"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    ".": {
      "solid": {
        "development": "./dist/dev.jsx",
        "import": "./dist/index.jsx"
      },
      "development": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/dev.js"
        }
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./index.css": "./dist/index.css"
  },
  "typesVersions": {},
  "scripts": {
    "dev": "vite serve dev",
    "build": "tsup",
    "test": "concurrently pnpm:test:*",
    "test:client": "vitest",
    "test:ssr": "pnpm run test:client --mode ssr",
    "prepublishOnly": "pnpm build",
    "format": "prettier --ignore-path .gitignore -w \"src/**/*.{js,ts,json,css,tsx,jsx}\" \"dev/**/*.{js,ts,json,css,tsx,jsx}\"",
    "lint": "concurrently pnpm:lint:*",
    "lint:code": "eslint --ignore-path .gitignore --max-warnings 0 src/**/*.{js,ts,tsx,jsx}",
    "lint:types": "tsc --noEmit",
    "update-deps": "pnpm up -Li"
  },
  "dependencies": {
    "record-tuple": "^1.3.2",
    "solid-signals": "workspace:^",
    "solid-u": "workspace:^"
  },
  "peerDependencies": {
    "solid-js": "^1.6.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "class-c": "^0.0.10",
    "concurrently": "^8.2.0",
    "esbuild": "^0.18.15",
    "esbuild-css-modules-plugin": "^3.0.1",
    "esbuild-plugin-solid": "^0.5.0",
    "esbuild-sass-plugin": "^2.13.0",
    "eslint": "^8.45.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "jsdom": "^22.1.0",
    "prettier": "3.0.0",
    "sass": "^1.66.1",
    "solid-js": "^1.7.8",
    "tsup": "^7.1.0",
    "tsup-preset-solid": "^2.0.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vite-plugin-solid": "^2.7.0",
    "vitest": "^0.33.0"
  },
  "keywords": [
    "solid"
  ],
  "packageManager": "pnpm@8.6.0",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8.6.0"
  }
}
